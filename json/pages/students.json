{
  "type": "component",
  "title": "Student Information List",
  "htmlStructure": [
    {
      "element": "div",
      "class": "container mt-4",
      "children": [
        {
          "element": "div",
          "class": "d-flex align-items-center justify-content-between",  
          "children": [
            {
              "element": "h3",
              "text": "Student Information List",
              "class": "mb-3"
            },
            {
              "element": "button",
              "text": "Add Student",
              "class": "btn btn-primary mb-3", 
              "action": {
                  "type": "navigate",
                  "route": "/student/add"
              }
            }
          ]
        },
        {
          "element": "table",
          "class": "table table-bordered",
          "children": [
            {
              "element": "thead",
              "children": [
                {
                  "element": "tr",
                  "children": [
                    { "element": "th", "text": "Name", "class": "text-center" },
                    { "element": "th", "text": "Age", "class": "text-center" },
                    { "element": "th", "text": "Grade", "class": "text-center" },
                    { "element": "th", "text": "Enrolled", "class": "text-center" }
                  ]
                }
              ]
            },
            {
              "element": "tbody",
              "repeat": "data.students",
              "children": [
                {
                  "element": "tr",
                  "children": [
                    { "element": "td", "text": "{{item.name}}" },
                    { "element": "td", "text": "{{item.age}}" },
                    { "element": "td", "text": "{{item.grade}}" },
                    { "element": "td", "text": "{{item.enrolled}}" }
                  ]
                }
              ]
            }
          ]
        }
      ]
    }
  ]
}