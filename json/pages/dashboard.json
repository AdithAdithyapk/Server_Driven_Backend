{
  "type": "component",
  "title": "Admin Dashboard",
  "htmlStructure": [
    {
      "element": "div",
      "class": "container mt-4",
      "children": [
        {
          "element": "h3",
          "text": "Dashboard Overview",
          "class": "mb-3"
        },
        {
          "element": "div",
          "class": "row",
          "children": [
            {
              "element": "div",
              "class": "col-md-4",
              "children": [
                {
                  "element": "div",
                  "class": "card text-white bg-primary mb-3",
                  "children": [
                    {
                      "element": "div",
                      "class": "card-body",
                      "children": [
                        {
                          "element": "h5",
                          "text": "Total Students: {{data.stats.totalStudents}}",
                          "class": "card-title"
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "element": "div",
              "class": "col-md-4",
              "children": [
                {
                  "element": "div",
                  "class": "card text-white bg-success mb-3",
                  "children": [
                    {
                      "element": "div",
                      "class": "card-body",
                      "children": [
                        {
                          "element": "h5",
                          "text": "Faculty Members: {{data.stats.facultyCount}}",
                          "class": "card-title"
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "element": "div",
              "class": "col-md-4",
              "children": [
                {
                  "element": "div",
                  "class": "card text-white bg-info mb-3",
                  "children": [
                    {
                      "element": "div",
                      "class": "card-body",
                      "children": [
                        {
                          "element": "h5",
                          "text": "Upcoming Events: {{data.stats.upcomingEvents}}",
                          "class": "card-title"
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "element": "div",
          "class": "row",
          "children": [
            {
              "element": "div",
              "class": "col-md-6",
              "children": [
                {
                  "element": "div",
                  "class": "card text-white bg-warning mb-3",
                  "children": [
                    {
                      "element": "div",
                      "class": "card-body",
                      "children": [
                        {
                          "element": "h5",
                          "text": "Fees Collected: \u20b9{{data.stats.collectedFees}}",
                          "class": "card-title"
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "element": "div",
              "class": "col-md-6",
              "children": [
                {
                  "element": "div",
                  "class": "card text-white bg-dark mb-3",
                  "children": [
                    {
                      "element": "div",
                      "class": "card-body",
                      "children": [
                        {
                          "element": "h5",
                          "text": "Recent Notices: {{data.stats.recentNotices}}",
                          "class": "card-title"
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "element": "chart",
          "type": "bar",
          "data": {
            "labels": ["January", "February", "March", "April"],
            "datasets": [
              {
                "data": [65, 59, 80, 81],
                "label": "Sales",
                "backgroundColor": ["#42A5F5", "#66BB6A", "#FF7043", "#8E24AA"]
              }
            ]
          },
          "options": {
            "responsive": true,
            "maintainAspectRatio": false,
            "scales": {
              "xAxes": [{
                "ticks": {
                  "beginAtZero": true
                }
              }]
            }
          },
          "class": "chart-class"
        },
        {
          "element": "chart",
          "type": "pie",
          "data": {
            "labels": ["Red", "Blue", "Yellow", "Green"],
            "datasets": [
              {
                "data": [300, 50, 100, 150],
                "backgroundColor": ["#FF5733", "#42A5F5", "#FFEB3B", "#66BB6A"]
              }
            ]
          },
          "options": {
            "responsive": true,
            "maintainAspectRatio": false,
            "plugins": {
              "legend": {
                "position": "top"
              }
            }
          },
          "class": "chart-class"
        },
        {
          "element": "chart",
          "type": "line",
          "data": {
            "labels": ["January", "February", "March", "April"],
            "datasets": [
              {
                "data": [65, 59, 80, 81],
                "label": "Sales Over Time",
                "fill": false,
                "borderColor": "#42A5F5",
                "tension": 0.1
              }
            ]
          },
          "options": {
            "responsive": true,
            "maintainAspectRatio": false,
            "scales": {
              "x": {
                "beginAtZero": true
              }
            },
            "plugins": {
              "legend": {
                "display": true,
                "position": "top"
              }
            }
          },
          "class": "chart-class"
        }
                
      ]
    }
  ]
}